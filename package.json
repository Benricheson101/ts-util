{
  "name": "@benricheson101/util",
  "repository": {
    "url": "https://github.com/Benricheson101/ts-util"
  },
  "homepage": "https://github.com/Benricheson101/ts-util",
  "version": "1.5.4",
  "private": false,
  "description": "A personal collection of reusable utility functions and types",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf build tests .tsbuildinfo",
    "lint": "biome lint .",
    "fmt": "biome check . --write",
    "pretest": "tsc --outDir tests --noEmit false",
    "test": "node --disable-warning=ExperimentalWarning --test --enable-source-maps --experimental-test-coverage --no-experimental-strip-types './tests/**/*.js'",
    "prepare": "husky"
  },
  "keywords": [
    "util",
    "utility"
  ],
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@types/node": "^20.19.9",
    "husky": "^9.1.7",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "files": [
    "build",
    "!**/*.test.*",
    "!**/*.tsbuildinfo",
    "biome.json",
    "tsconfig.json",
    "LICENSE",
    "templates"
  ],
  "sideEffects": false,
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./build/index.d.mts",
        "default": "./build/index.mjs"
      },
      "require": {
        "types": "./build/index.d.ts",
        "default": "./build/index.js"
      }
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "esbuild"
    ]
  }
}
